{
  "numStartups": 1,
  "primaryApiKey": "{{ anthropic_api_key }}",
  "customApiKeyResponses": {
    "approved": ["{{ anthropic_api_key.split('-', 3)[-1] }}"],
    "rejected": []
  },
  "bypassPermissionsAccepted": true,
  "bypassPermissionsModeAccepted": true,
  "hasCompletedOnboarding": true,
  "projects": {
    "/home/{{ ai_hub_user }}": {
      "allowedTools": [],
      "mcpContextUris": [],
      "mcpServers": {
        "memory": {
          "command": "node",
          "args": ["{{ mcp_servers_path }}/mcp-memory/dist/index.js"],
          "env": {
            "OPENAI_API_KEY": "{{ openai_api_key }}",
            "QDRANT_URL": "{{ qdrant_url }}",
            "SQLITE_DB_PATH": "{{ sqlite_db_path }}"
          }
        },
        "proxmox": {
          "command": "node",
          "args": ["{{ mcp_servers_path }}/mcp-proxmox/index.js"],
          "env": {
            "PROXMOX_HOST": "{{ proxmox_host }}",
            "PROXMOX_USER": "{{ proxmox_user }}",
            "PROXMOX_TOKEN_NAME": "{{ proxmox_token_name }}",
            "PROXMOX_TOKEN_VALUE": "{{ proxmox_token_value }}",
            "PROXMOX_ALLOW_ELEVATED": "{{ proxmox_allow_elevated }}",
            "PROXMOX_PORT": "{{ proxmox_port }}"
          }
        }{% if unifi_password is defined and unifi_password | length > 0 %},
        "unifi": {
          "command": "{{ mcp_servers_path }}/unifi-network-mcp/.venv/bin/unifi-network-mcp",
          "args": [],
          "env": {
            "UNIFI_HOST": "{{ unifi_host }}",
            "UNIFI_USERNAME": "{{ unifi_username }}",
            "UNIFI_PASSWORD": "{{ unifi_password }}",
            "UNIFI_PORT": "{{ unifi_port }}",
            "UNIFI_SITE": "{{ unifi_site }}",
            "UNIFI_VERIFY_SSL": "{{ unifi_verify_ssl }}"
          }
        }{% endif %}
      },
      "enabledMcpjsonServers": [],
      "disabledMcpjsonServers": [],
      "hasTrustDialogAccepted": true,
      "bypassPermissionsAccepted": true,
      "projectOnboardingSeenCount": 1,
      "hasClaudeMdExternalIncludesApproved": false,
      "hasClaudeMdExternalIncludesWarningShown": false,
      "exampleFiles": [],
      "reactVulnerabilityCache": {
        "detected": false,
        "package": null,
        "packageName": null,
        "version": null,
        "packageManager": null
      }
    }
  },
  "userID": "automated-deployment",
  "firstStartTime": "{{ ansible_date_time.iso8601 }}",
  "hasShownOpus45Notice": {},
  "officialMarketplaceAutoInstallAttempted": true,
  "officialMarketplaceAutoInstalled": true
}
